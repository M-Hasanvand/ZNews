    <!--swal2-->
    <script src="~/lib/Sweetalert2/Sweetalert2.js"></script>
    <link href="~/lib/Sweetalert2/Sweetalert2.css" rel="stylesheet" />
    <!--Scripts-->
    <script>
        function login() {
            var email = $("#email").val();
            var nationalNumber = $("#nationalNumber").val();
            var password = $("#password").val();
            var postData = {
                'Email': email,
                'NationalNumber': nationalNumber,
                'Password': password
            };
            $.ajax({
                contentType: "application/x-www-form-urlencoded",
                type: "POST",
                dataType:"JSON",
                url: "/Admin/Authentication/Sginin",
                data: postData,
                success: function (data) {
                    if (data.isSuccess == true) {
                        swal.fire('موفق', data.message, 'success').then(() => { window.location.replace("/Admin/Home/Index"); });
                    } else {
                        swal.fire('ناموفق', data.message, 'warning');
                    }
                }
            });
        }
    </script>
